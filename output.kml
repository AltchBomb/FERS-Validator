    </LookAt>
    <Point>
        <coordinates>-18.46,134.399,0</coordinates>
        <altitudeMode>clampToGround</altitudeMode>
    </Point>
</Placemark>
<Placemark>
    <name>SurvRx</name>
    <description></description>
    <styleUrl>#receiver</styleUrl>
    <LookAt>
        <longitude>-18.46</longitude>
        <latitude>134.399</latitude>
        <altitude>0</altitude>
        <heading>-148.4122922628044</heading>
        <tilt>40.5575073395506</tilt>
