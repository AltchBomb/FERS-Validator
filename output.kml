    </Point>
</Placemark>
<Placemark>
    <name>Aeroplane</name>
    <description></description>
    <styleUrl>#target</styleUrl>
    <gx:Track>
        <altitudeMode>clampToGround</altitudeMode>
        <when>0</when>
        <gx:coord>-19.1072 134.399 0</gx:coord>
        <when>50</when>
        <gx:coord>-19.2495 134.399 0</gx:coord>
    </gx:Track>
</Placemark>
<Placemark>
    <name>Start: Aeroplane</name>
    <styleUrl>#target</styleUrl>
