<Folder>
  <name>Reference Coordinate</name>
  <description>Placemarks for various elements in the FERSXML file. All Placemarks are situated relative to this reference point.</description>
  <LookAt>
    <longitude>-18.46</longitude>
    <latitude>133.95</latitude>
    <altitude>0</altitude>
    <heading>-148.4122922628044</heading>
    <tilt>40.5575073395506</tilt>
    <range>10000</range>
  </LookAt>
<Placemark>
    <name>PrieskaTx</name>
    <description></description>
    <styleUrl>#transmitter</styleUrl>
    <LookAt>
        <longitude>-18.46</longitude>
        <latitude>133.95</latitude>
        <altitude>1000</altitude>
        <heading>-148.4122922628044</heading>
        <tilt>40.5575073395506</tilt>
        <range>500.6566641072245</range>
    </LookAt>
    <Point>
        <coordinates>-18.46,133.95,1000</coordinates>
        <altitudeMode>relativeToGround</altitudeMode>
        <extrude>1</extrude>
    </Point>
</Placemark>
<Placemark>
    <name>RefRx</name>
    <description></description>
    <styleUrl>#receiver</styleUrl>
    <LookAt>
        <longitude>-18.46</longitude>
        <latitude>134.399</latitude>
        <altitude>0</altitude>
        <heading>-148.4122922628044</heading>
        <tilt>40.5575073395506</tilt>
